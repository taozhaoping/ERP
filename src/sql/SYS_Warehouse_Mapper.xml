<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="M_Warehouse">
	<resultMap id="T_Warehouse" type="Warehouse">
		<id column="ID" property="id" />
		<result column="NAME" property="name" jdbcType="VARCHAR" />
		<result column="ADDRESS" property="address" jdbcType="VARCHAR" />
		<result column="ENTERPRISE_ID" property="enterpriseID"/>
		<result column="enabled" property="enabled" />
	</resultMap>
	<sql id="Base_Column_List">
		ID, NAME, ADDRESS,ENTERPRISE_ID,enabled
	</sql>
	<sql id="where">
		<where>
			<if test="id != null">
				and id = #{id}
			</if>
			<if test="name != null">
				and name = #{name,jdbcType=VARCHAR}
			</if>
			<if test="address != null">
				and address = #{address,jdbcType=VARCHAR}
			</if>
			<if test="enterpriseID != null">
				and ENTERPRISE_ID = #{enterpriseID}
			</if>
			<if test="enabled != null">
				and enabled = #{enabled}
			</if>
		</where>
	</sql>
	
	<select id="select" resultMap="T_Warehouse" parameterType="Warehouse">
		select
		<include refid="Base_Column_List" />
		from SYS_WAREHOUSE
		<include refid="where" />
	</select>
	
	<select id="list" resultMap="T_Warehouse" parameterType="Warehouse">

		select
		<include refid="Base_Column_List" />
		from SYS_WAREHOUSE
		<include refid="where" />
		<if test="orderByClause != null">
			order by ${orderByClause}
		</if>
	</select>
	
	<select id="count" resultType="Integer" parameterType="Warehouse">
		select
		count(id)
		from SYS_WAREHOUSE
		<include refid="where" />
	</select>

	<insert id="insert" parameterType="Warehouse" >
		<selectKey keyProperty="id" resultType="Integer" order="BEFORE">
			select
			SEQUENCE_SYS_WAREHOUSE.nextval from dual
		</selectKey>
		insert into SYS_WAREHOUSE
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id != null">
				ID,
			</if>
			<if test="name != null">
				NAME,
			</if>
			<if test="address != null">
				ADDRESS,
			</if>
			<if test="enterpriseID != null">
				ENTERPRISE_ID,
			</if>
			<if test="enabled != null">
				enabled,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null">
				#{id},
			</if>
			<if test="name != null">
				#{name,jdbcType=VARCHAR},
			</if>
			<if test="address != null">
				#{address,jdbcType=VARCHAR},
			</if>
			<if test="enterpriseID != null">
				#{enterpriseID},
			</if>
			<if test="enabled != null">
				#{enabled},
			</if>
		</trim>
	</insert>
	<update id="update" parameterType="Warehouse">
		update SYS_WAREHOUSE
		<set>
			<if test="name != null">
				NAME = #{name,jdbcType=VARCHAR},
			</if>
			<if test="address != null">
				ADDRESS = #{address,jdbcType=VARCHAR},
			</if>
			<if test="enterpriseID != null">
				ENTERPRISE_ID = #{enterpriseID},
			</if>
			<if test="enabled != null">
				enabled = #{enabled},
			</if>
		</set>
		where ID = #{id}
	</update>
</mapper>